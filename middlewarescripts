    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'test'
            }
        }
        stage('test') {
            steps {
                echo 'mangae'
            }
        }
        stage('build') {
            steps {
                echo 'deploy'
            }
        }
        stage('deploy') {
            steps {
                echo 'done'
            }
        }
    }
}
